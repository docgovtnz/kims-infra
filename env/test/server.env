########################
#      Server
########################

# The release we want to install the next time CDK is run
SERVER_RELEASE=0.0.67

# This Prefix used to separate and clarify the name of the application
APP_NAME_PREFIX=test-kims

# This is the name prefix of where the API will be exposed  (e.g; api.base-stack.net)
API_DOMAIN_PREFIX=api

# This is the "core" domain name that will be built on top of by the other names below
BASE_DOMAIN_NAME=test.kims.doc.govt.nz

# API Certificate ARN
API_CERTIFICATE_ARN=arn:aws:acm:us-east-1:448611289225:certificate/cfed9f43-f5ab-43f1-bcb9-f45d6b45879a

# This is the AWS lookup ID of the APP_BASE_DOMAIN_NAME above
HOSTED_ZONE_ID=Z08602002UX1RDDP23E5X

# VPC of where Lambda will be created, must be the same as the Database
VPC_ID=vpc-071ba963

# Comma separate list of subnets (must be within VPC)
SUBNET_ID_LIST=subnet-da5ce483,subnet-a15330d7,subnet-2921584d

# Comma separated list of security-groups (must be within VPC, may only be one)
SECURITY_GROUP_LIST=sg-55a63032

# ECR Name of where the Docker images will be stored
ECR_REPOSITORY_NAME=kims-server

# The S3 bucket where the meta files are stored
META_BUCKET_NAME=test-kims-meta

# Database connection settings. If Password is a AWS:ARN then it will do a "Secrets" lookup to get the value.
DATABASE_HOST=test-kims-db.cluster-cypaujriu8hi.ap-southeast-2.rds.amazonaws.com
DATABASE_PORT=5432
DATABASE_USER=postgres
DATABASE_PASSWORD=arn:aws:secretsmanager:ap-southeast-2:448611289225:secret:test/kims/database/password-KbDEWN
DATABASE_PASSWORD_KEY=test-kims-db-password
DATABASE_NAME=test-kims-db

# Provider responsible for Authentication, will switch between different alternative property sets below
AUTHENTICATION_PROVIDER=COGNITO
AUTHENTICATION_PUBLIC_KEY_URL=https://cognito-idp.ap-southeast-2.amazonaws.com/ap-southeast-2_CIl5VvfWD/.well-known/jwks.json
AUTHENTICATION_ISSUER=https://cognito-idp.ap-southeast-2.amazonaws.com/ap-southeast-2_CIl5VvfWD


# Switch off Color logging for AWS Cloudwatch logs.
NO_COLOR=true